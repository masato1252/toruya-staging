<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      <h4 class="modal-title">
        <%= t("dashboard.modal.shop_dashboard.staff.upgrade_title") %>
      </h4>
    </div>
    <div class="modal-body">
      <%= t("dashboard.modal.shop_dashboard.staff.upgrade_content", shop_name: @warning_shop.display_name) %>
    </div>
    <div class="modal-footer centerize">
      <%= link_to t("action.cancel_upgrade"), "#", class: "btn btn-tarco", data: { dismiss: "modal" } %>
    </div>
  </div>
</div>
