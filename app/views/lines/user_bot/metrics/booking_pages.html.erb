<div class="container-fluid">
  <div class="row">
    <div class="col-sm-6 px-0 settings-view">
      <div class="booking-options form with-top-bar">
        <%= render "lines/user_bot/top_navigation_bar", title: I18n.t("user_bot.dashboards.metrics.title") %>

        <div class="field-header"><%= I18n.t("user_bot.dashboards.services.service_page") %></div>
        <div class="booking-pages-list">
          <% @booking_pages.each do |page| %>
            <%= link_to lines_user_bot_metrics_booking_page_path(page, business_owner_id: business_owner_id), class: "field-row with-next-arrow" do %>
              <div>
                <h3><%= page.title %></h3>
              </div>
              <i class="fa fa-angle-right"></i>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>

    <div class="col-sm-6 px-0 hidden-xs preview-view"></div>
  </div>
</div>
