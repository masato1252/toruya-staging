<div class="booking-pages form with-top-bar">
  <%= render "lines/user_bot/top_navigation_bar", title: I18n.t("user_bot.dashboards.metrics.title") %>

  <%= react_component("user_bot/metrics/sale_pages_visits", {
    demo: params[:demo],
    metric_path: online_service_sale_pages_visits_lines_user_bot_metrics_path({ id: params[:id], format: "json" }),
  }, { camelize_props: false }) %>
  <%= react_component("user_bot/metrics/sale_pages_conversions", {
    demo: params[:demo],
    metric_path: online_service_sale_pages_conversions_lines_user_bot_metrics_path({ id: params[:id], format: "json" }),
  }, { camelize_props: false }) %>
</div>
