<%= react_component("user_bot/booking_pages/custom_messages/edit", {
  props: {
    business_owner_id: business_owner_id,
    scenario: params[:scenario],
    message: {
      id: @message&.id,
      service_id: @booking_page.id,
      service_type: @booking_page.class.name,
      before_minutes: @message&.before_minutes,
      after_days: @message&.after_days,
      template: @template.presence || '',
    }.merge!(@booking_page.message_template_variables(current_user))
  }
},
{ camelize_props: false }
) %>
