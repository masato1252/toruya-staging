<%= react_component("user_bot/services/creation_flow", {
  props: {
    service_goals: Lines::UserBot::ServicesController::GOALS,
    companies: super_user.shops.includes({ logo_attachment: :blob }).map { |s| ShopSerializer.new(s).attributes_hash.merge!(type: "Shop") }.push(
      ShopProfileSerializer.new(super_user.profile).attributes_hash.merge!(type: "Profile")
    ),
    upsell_services: super_user.sale_pages.includes(:product).map {|sale_page| SalePageOptionSerializer.new(sale_page).attributes_hash }
  }
},
{ camelize_props: false }
) %>
