<div id="login">
  <h2><%= I18n.t("common.login") %></h2>
  <main id="socialLogin">
    <p><%= t("common.toruya_user_login_html") %></p>

    <%= button_to toruya_line_login_url(root_path, staff_token: params[:staff_token], consultant_token: params[:consultant_token]), class: "btn line-button with-wording only-word" do %>
