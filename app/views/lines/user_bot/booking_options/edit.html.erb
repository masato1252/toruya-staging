<%= react_component("user_bot/booking_options/edit", {
  props: {
    business_owner_id: business_owner_id,
    menu_group_options: menu_group_options(@menu_result[:category_with_menu_options], :minutes),
    editing_menu: @editing_menu,
    ticket_expire_date_desc_path: asset_pack_path("media/assets/user_bot/ticket_expire_date_desc.png"),
    booking_option: @booking_option.attributes.merge!(
      amount_cents: @booking_option.amount&.fractional,
      price_type: @booking_option.ticket_enabled? ? "ticket" : "regular",
      start_at_type: @booking_option.start_at ? "date" : "now",
      start_at_date_part: @booking_option.start_at_date || Date.current.to_s,
      start_at_time_part: @booking_option.start_at_time,
      end_at_type: @booking_option.end_at ? "date" : "now",
      end_at_date_part: @booking_option.end_at_date || Date.current.to_s,
      end_at_time_part: @booking_option.end_at_time,
    ),
    attribute: @attribute,
    i18n: {
    }.merge!([
      t("settings.booking_option.form"),
      t("action").slice(:save),
      t("common").slice(:required_time, :select_a_menu, :minute),
      {
        page_title: t("settings.booking_option.form.#{@attribute}_title"),
        top_bar_header: translation_resuce { I18n.t("settings.booking_option.form.#{@attribute}_top_bar_header", raise: true) },
        hint: translation_resuce { I18n.t("settings.booking_option.form.#{@attribute}_hint", raise: true) },
      },
    ].inject(:merge))
  }
},
{ camelize_props: false }
) %>
