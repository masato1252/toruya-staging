<%= react_component("user_bot/settings/shops/edit", {
  props: {
    business_owner_id: business_owner_id,
    shop: CompanyInfoSerializer.new(@shop).attributes_hash.merge!(@shop.slice(:name, :address_details)),
    attribute: params[:attribute],
    business_schedules: @business_schedules&.map { |business_schedule| BusinessScheduleSerializer.new(business_schedule).attributes_hash },
    previous_path: @previous_path,
    title: @title,
    header: @header
  }
},
{ camelize_props: false }
) %>
