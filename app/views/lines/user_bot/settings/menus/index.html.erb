<div class="settings-dashboard form with-top-bar">
  <%= render "lines/user_bot/top_navigation_bar", title: t("common.menu") %>
  <div class="field-header"><%= t("common.menu") %></div>
  <% @menus.each do |menu| %>
    <%= link_to lines_user_bot_settings_menu_path(business_owner_id, menu.id), class: "field-row" do %>
      <span><%= menu.name %></span><i class="fa fa-angle-right"></i>
    <% end %>
  <% end %>
</div>
