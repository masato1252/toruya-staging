<div id="shopNav" class="contents">
  <%= react_component("Post", { title: "Hello" }) %>
  <h2><%= t("dashboard.my_shop") %></h2>
  <nav>
    <% owning_shop_options.each do |option| %>
      <div>
        <%= link_to option.shop.display_name, shop_reservations_path(option.shop), class: "BTNorange" %>
      </div>
    <% end %>
    <%= link_to settings_path(current_user) do %>
      <i class="fa fa-cog fa-4x" aria-hidden="true"></i>
      設定
    <% end %>
  </nav>
</div>

<div id="shopNav" class="contents">
  <h2><%= t("dashboard.shop_you_work_for") %></h2>
  <nav>
    <% working_shop_options.each do |option| %>
      <div>
        <%= link_to option.shop.display_name, shop_reservations_path(option.shop), class: "BTNorange" %>
      </div>
    <% end %>
  </nav>
</div>
