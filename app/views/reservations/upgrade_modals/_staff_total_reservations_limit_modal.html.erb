<div class="modal fade" id="staffUpgradeTotalReservationsLimitModal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">
          <%= t("reservation.modal.staff.total_reservations_limit.upgrade_title") %>
        </h4>
      </div>
      <div class="modal-body">
        <%= t("reservation.modal.staff.total_reservations_limit.upgrade_content").html_safe %>
      </div>
      <div class="modal-footer centerize">
        <%= link_to t("action.cancel_upgrade"), "#", class: "btn BTNtarco", data: { dismiss: "modal" } %>
      </div>
    </div>
  </div>
</div>
