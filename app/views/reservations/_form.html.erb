<%= react_component("UI.Reservation.Form",
                    { reservation: reservation.attributes.merge(
                      start_time_date_part: date,
                      start_time_time_part: start_time,
                      end_time_time_part: end_time,
                      staff_ids: reservation.staff_ids.map(&:to_s),
                      customers: reservation.customers.map { |c| { label: c.name, value: c.id, level: c.state } }),
                      min_staffs_number: @result.try(:[], :selected_menu).try(:min_staffs_number),
                      max_seat_number: @result.try(:[], :selected_menu).try(:max_seat_number),
                      menu_options: menu_options(@result.try(:[], :menus)),
                      staff_options: staff_options(@result.try(:[], :staffs), @result.try(:[], :selected_menu)),
                      start_time_restriction: @time_ranges.try(:first).try(:to_s, :simple_time),
                      end_time_restriction: @time_ranges.try(:last).try(:to_s, :simple_time),
                      available_times_path: times_shop_reservations_available_options_path(shop),
                      available_menus_path:  menus_shop_reservations_available_options_path(shop),
                      available_staffs_path: staffs_shop_reservations_available_options_path(shop),
                      customer_add_path: shop_customers_path(shop),
                      form_authenticity_token: form_authenticity_token,
                      reservation_path: reservation_path
} ) %>
