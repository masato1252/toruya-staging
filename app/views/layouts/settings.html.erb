<!DOCTYPE html>
<html>
  <head>
    <title>Toruyaースモールビジネスの顧客管理ツールー</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= javascript_pack_tag "application" %>
  </head>

  <body class="setting firstSetting <%= body_class %>" data-controller="atomic-requests">
    <%= render "layouts/dashboard_header_bar" %>
    <div class="contents">
      <%= render "layouts/settings_sider" %>
      <div id="content">
        <%= flash_messages %>
        <%= yield %>
        <%= render "layouts/shop_dashboard_upgrade_modal" %>
        <%= render "layouts/filter_upgrade_modal" %>
      </div>
    </div>
  </body>
</html>
