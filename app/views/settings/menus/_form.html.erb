<div id="about">
  <p>Please enter menu informations here.<br>Toruya will accept reservation based on this setting.</p>
</div>

<%= react_component("UI.Settings.MenuForm", {
  save_menu_path: url,
  cancel_path: settings_menus_path,
  menu: menu,
  reservation_settings: reservation_setting_options(super_user.reservation_settings),
  selected_reservation_setting: menu.reservation_setting,
  selected_reservation_setting_rule: menu.menu_reservation_setting_rule,
  shops: react_attributes(super_user.shops),
  selected_shops: react_attributes(menu.shops),
  staffs: react_attributes(super_user.staffs),
  selected_staffs: react_attributes(menu.staffs),
  staff_menus: react_attributes(menu.staff_menus),
  form_authenticity_token: form_authenticity_token
})%>
