<%= react_component("UI.Customers.Filter.Dashboard", {
  regions: regions,
  contact_group_options: contact_group_options,
  year_options: year_options,
  month_options: month_options,
  day_options: day_options,
  living_place_query_type_options: [{ label: "inside of", value: true }, { label: "outside of", value: false }],
  date_query_options: [
    { label: "on", value: "on"},
    { label: "before", value: "before"},
    { label: "after", value: "after"},
    { label: "between", value: "between"}
  ],
  yes_no_options: [ { label: "Have", value: true }, { label: "No", value: false }],
  menu_group_options: menu_group_options(@menu_result[:category_with_menu_options]),
  menu_options: menu_group_options(@menu_result[:menu_options]),
  staff_options: staff_options(@staff_options, []),
  reservation_before_checked_in_state_options: Reservation::BEFORE_CHECKED_IN_STATES.map{ |state|
    { label: I18n.t("reservation.#{state}"), value: state }
  },
  reservation_after_checked_in_state_options: Reservation::AFTER_CHECKED_IN_STATES.map{ |state|
    { label: I18n.t("reservation.#{state}"), value: state }
  },
  saved_filter_options: @filters.map{ |filter| { label: filter.name, value: filter.id } },
  filtered_outcome_options: @filtered_outcomes.map{ |outcome|
    {
      id: outcome.id,
      name: "#{outcome&.filter&.name} #{outcome.created_at.to_s(:date)}",
      file_url: outcome.file.url,
      state: outcome.aasm_state
    } },
  printing_page_size_blank_option: I18n.t("customer.printing_page_size.paper_size"),
  printing_page_size_options: Customers::PrintingConfig::PAGE_SIZE.map { |key, page_size| { label: page_size[:name], value: key} },
  printing_path: customer_printing_index_path(format: :pdf),
  filter_path: customer_user_filter_index_path(super_user),
  save_filter_path: customer_user_saved_filters_path(super_user),
  fetch_filter_path: fetch_customer_user_saved_filters_path(super_user),
  delete_filter_path: delete_customer_user_saved_filters_path(super_user),
  form_auth_token: form_authenticity_token,
  home_label: I18n.t("customer.home"),
  work_label: I18n.t("customer.work"),
  mobile_label: I18n.t("customer.mobile"),
  select_region_label: I18n.t("customer.select_region"),
  select_year_label: I18n.t("customer.select_year"),
  select_month_label: I18n.t("customer.select_month"),
  select_day_label: I18n.t("customer.select_day"),
  select_menu_label: " Please select a menu",
  select_staff_label: "Please select a staff",
  processing_message: I18n.t("common.processing")
}) %>
